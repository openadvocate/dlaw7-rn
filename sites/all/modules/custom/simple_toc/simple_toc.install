<?php

/**
 * @file
 * Install, update and uninstall functions for the Simple Table of Contents module.
 */

/**
 * Implements hook_uninstall().
 */
function simple_toc_uninstall() {
  // Remove variables
  variable_del('simple_toc_start_level');
  variable_del('simple_toc_depth');
  variable_del('simple_toc_title');
}
